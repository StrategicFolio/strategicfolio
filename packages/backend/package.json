{
  "name": "@strategicfolio/api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "watch": "tsc --watch",
    "start:prod": "tsc && node dist/app.js",
    "start": "nodemon --watch \"src/**\" --ext \"ts,js,json\" --exec \"tsc && ts-node src/app.ts\"",
    "migration:generate": "node ./scripts/migration-generate.js",
    "migration:revert": "typeorm-ts-node-esm migration:revert -d ./src/database/dataSource.ts",
    "migration:run": "typeorm-ts-node-esm migration:run -d ./src/database/dataSource.ts"
  },
  "dependencies": {
    "@strategicfolio/common": "0.0.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
