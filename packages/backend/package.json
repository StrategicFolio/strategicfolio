    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.182",
    "@types/luxon": "^2.3.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.0.3",
    "@types/node-fetch": "^2.6.2",
    "@types/nodemailer": "^6.4.4",
    "@types/passport": "^1.0.9",
    "@types/passport-jwt": "^3.0.6",
    "concurrently": "^7.2.2",
    "nodemon": "^2.0.19",
