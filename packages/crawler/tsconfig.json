{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictNullChecks": false,
    "noImplicitAny": true,
    "alwaysStrict": true,
    "noImplicitOverride": false,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noEmit": true
  }
}
